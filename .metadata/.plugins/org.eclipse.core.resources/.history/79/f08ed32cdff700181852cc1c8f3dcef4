package com.balde.bean;

import lombok.*;

@Getter
@Setter
@NoArgsConstructor
//@AllArgsConstructor
public class User {
	
	private int id;
	private String firstName;
	private String lastName;
	private int age;
	
	public User(int id, String firstName, String lastName, int age) {
		super();
		this.id = id;
		this.firstName = firstName;
		this.lastName = lastName;
		this.age = age;
	}
	
	
}
