package com.balde.bean;

public class User {
	
	private Long id;
	private String firstName;
	private String lastName;
	private int age;
	
	public User() {
		super();
	}

	public User(Long id, String firstName, String lastName, int age) {
		super();
		this.id = id;
		this.firstName = firstName;
		this.lastName = lastName;
		this.age = age;
	}
	
	
}
